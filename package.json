{
  "name": "vite-react-ts-rtk",
  "private": true,
  "version": "2.1.1",
  "type": "module",
  "scripts": {
    "start": "vite --open --mode=dev",
    "dev": "vite --mode=dev",
    "build": "tsc && yarn test && vite build --mode=prod",
    "preview": "vite preview",
    "format": "prettier --config ./.prettierrc --write \"{src,test}/**/*.{ts,tsx,scss}\"",
    "formatcheck": "prettier --config ./.prettierrc --check \"{src,test,conf}/**/*.{ts,tsx,scss}\"",
    "api": "node scripts/api.cjs && yarn format && yarn eslint:fix",
    "eslint": "eslint \"{src,test}/**/*.{ts,tsx}\" --report-unused-disable-directives",
    "eslint:fix": "eslint \"{src,test}/**/*.{ts,tsx}\" --fix",
    "test": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@types/node": "^24.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.2",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@nabla/vite-plugin-eslint": "^2.0.5",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "eslint": "^9.29.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "59.0.1",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "prettier": "^3.6.0",
    "sass": "1.89.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "yarn@4.9.2"
}
